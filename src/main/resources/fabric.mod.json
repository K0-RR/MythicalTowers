{
    "schemaVersion": 1,
    "id": "mythicaltowers",
    "version": "${version}",
    "name": "MythicalTowers",
    "description": "An adventure mod adding new dungeons with unique loot, items, blocks etc.",
    "authors": [
        "RedGrapefruit09"
    ],
    "contact": {
        "repo": "https://github.com/RedGrapefruit09/MythicalTowers"
    },
    "license": "MIT",
    "icon": "assets/mythicaltowers/icon.png",
    "environment": "*",
    "entrypoints": {
        "client": [
            {
                "adapter": "kotlin",
                "value": "com.redgrapefruit.mythicaltowers.client.MythicalTowersClient"
            }
        ],
        "main": [
            {
                "adapter": "kotlin",
                "value": "com.redgrapefruit.mythicaltowers.common.MythicalTowers"
            }
        ]
    },
    "mixins": [
        "mythicaltowers.mixins.json"
    ],
    "depends": {
        "fabricloader": ">=0.11.3",
        "fabric": "*",
        "fabric-language-kotlin": ">=1.6.0+kotlin.1.5.0",
        "minecraft": "1.16.5"
    }
}
